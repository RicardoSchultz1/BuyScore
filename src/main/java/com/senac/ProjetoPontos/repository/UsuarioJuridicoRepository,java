package com.senac.ProjetoPontos.repository;

import java.util.Optional;
import java.util.UUID;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;

import com.senac.ProjetoPontos.Model.UsuarioJuridicoModel;

public interface UsuarioJuridicoRepository extends JpaRepository<UsuarioJuridicoModel, UUID>{
	
	@Query(nativeQuery = true, value = "Select * from usuario_juridico where id = :id")
    Optional<UsuarioJuridicoModel> findById (int id);
}
